@model webMVC.Models.TaiKhoan

@{
    ViewBag.Title = "Details";
}


<div id="main-wrapper">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-white">
                <div class="panel-heading clearfix">
                    <h4 class="panel-title">Thông tin chi tiết loại sản phẩm</h4>
                </div>
                <div class="panel-body">
                    <div class="form-group">
                        <label for="exampleInputEmail1">Họ</label>
                        <div class="form-control">@Html.DisplayFor(model => model.Ho)</div>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Tên</label>
                        <div class="form-control">@Html.DisplayFor(model => model.Ten)</div>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Ngày sinh</label>
                        <div class="form-control">@Html.DisplayFor(model => model.Ngaysinh)</div>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Số điện thoại</label>
                        <div class="form-control">@Html.DisplayFor(model => model.Sdt)</div>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Email</label>
                        <div class="form-control">@Html.DisplayFor(model => model.Email)</div>
                    </div>
                    <a class="btn btn-success" href="/admin/TaiKhoans/Edit/@Model.makh">Sửa</a>
                    <a class="btn btn-flickr" href="/admin/TaiKhoans">quay lại</a>
                </div>
            </div>
        </div>
    </div>
</div>
